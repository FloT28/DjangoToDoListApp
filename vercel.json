{
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "vercel-python@3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myproject/wsgi.py"
    }
  ],
    "functions": {
    "myproject/wsgi.py": {
      "runtime": "vercel-python@3.10",
      "maxLambdaSize": "15mb"
    }
  }
}